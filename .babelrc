{
  "presets": [
    "es2015",
    "stage-1",
    "react",
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-function-bind",
    ["lodash", { "id": ["lodash", "semantic-ui-react"] }]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ],
    },
    "production": {
      "plugins": [
        ["transform-es2015-modules-systemjs", {
          "systemGlobal": "SystemJS"
        }]
      ]
    }
  }
}